
<%- include('../inc/top') %>

            
            <h3 class="listU">CATEGORY</h3>
            <% if(msg.length>0){ %>
                <p class="alert alert-info"> <%= msg %></p>
             <% } %>
            
        <div class="col-12">
            <table class="table table-striped">
                <thead>
                    <tr>
                        <th  scope="col">ID</th>
                        <th scope="col">Category</th>
                        

                    </tr>
                </thead>
                <tbody>

                    <% cats.forEach(function(c) { %>
                        <tr> 
                            <th scope="row"><%= c._id %></th>
                            <td><%= c.name %></td>
                            
                            
                            <td> 
                                <a href="/category/edit/<%= c._id %>" class="btn bg-success-subtle">Edit</a>
                                <a href="/category/delete/<%= c._id %>" class="btn bg-dark-subtle">Delete</a>
                                
                            </td>
                        </tr>
                    <% }); %>
                    
                    
                    
                </tbody>
            </table>
        </div>

